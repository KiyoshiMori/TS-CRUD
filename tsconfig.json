{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}